<%= form_with(model: group) do |form| %>
  <%= render "shared/errors", object: group %>

  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :group_type %>
    <%= form.text_field :group_type %>
  </div>

  <div>
    <%= form.label :code %>
    <%= form.text_field :code %>
  </div>

  <div>
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>

  <div>
    <%= form.label :phone %>
    <%= form.text_field :phone %>
  </div>

  <div>
    <%= form.label :active %>
    <%= form.check_box :active %>
  </div>

  <div class="pt-5">
    <%= form.submit %>
  </div>
<% end %>
